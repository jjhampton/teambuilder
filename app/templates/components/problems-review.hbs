<h1 class="primary-heading">Review Your Team's Performance</h1>
<h2 class="secondary-heading">{{model.name}}</h2>
<ul class="review-members-list">
  {{teammate-review model=model.owner problem=model action="reviewTeammate"}}
  {{#if model.member1}}
    {{teammate-review model=model.member1  action="reviewTeammate"}}
  {{/if}}
  {{#if model.member2}}
    {{teammate-review model=model.member2 action="reviewTeammate"}}
  {{/if}}
  {{#if model.member3}}
    {{teammate-review model=model.member3 action="reviewTeammate"}}
  {{/if}}
  {{#if model.member4}}
    {{teammate-review model=model.member4 action="reviewTeammate"}}
  {{/if}}
  {{#if model.member5}}
    {{teammate-review model=model.member5 action="reviewTeammate"}}
  {{/if}}
</ul>

{{yield}}
